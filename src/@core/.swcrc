{
  "jsc": {
      "parser": {
          "syntax": "typescript",
          "decorators": true
      },
      "baseUrl": "./src",
      "target": "es2017",
      "paths": {
          "@seedwork/*": ["@seedwork/*"],
          "@clients/*": ["clients/*"]
      },
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      }
  },
  "module": {
    "type": "commonjs"
  }
}
